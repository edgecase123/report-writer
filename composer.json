{
  "name": "jenkowitz/report-writer",
  "description": "A banded report writer for PHP applications with fluent group aggregates",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Lee Jenkins",
      "email": "edgecase123@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4 || ^8.0",
    "doctrine/orm": "^2.7",
    "ext-json": "*",
    "symfony/twig-bundle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^5.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ReportWriter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReportWriter\\Tests\\": "tests/"
    }
  }
}
